<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="570.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerSheetController">
	<children>
		<Label alignment="CENTER" contentDisplay="CENTER" layoutX="199.0" layoutY="24.0" text="Customer Summary" textAlignment="CENTER">
			<font>
				<Font name="Comic Sans MS" size="18.0" />
			</font>
		</Label>
		<TableView fx:id="customerTable" layoutX="37.0" layoutY="67.0" prefHeight="421.0" prefWidth="487.0">
			<columns>
				<TableColumn fx:id="customerTag" prefWidth="121.0" text="Customer Tag" />
				<TableColumn fx:id="time" prefWidth="118.0" text="Time (minutes)" />
				<TableColumn fx:id="event" prefWidth="99.0" text="Event" />
				<TableColumn fx:id="customerType" prefWidth="148.0" text="Customer Type" />
			</columns>
		</TableView>
      <Button fx:id="nextButton" layoutX="216.0" layoutY="510.0" mnemonicParsing="false" onAction="#nextButton" text="Next" />
      <Button fx:id="exitButton" layoutX="280.0" layoutY="510.0" mnemonicParsing="false" onAction="#exitButton" prefHeight="25.0" prefWidth="49.0" text="Exit" />
	</children>
</AnchorPane>
